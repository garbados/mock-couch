{
  "name": "mock-couch",
  "version": "0.1.9",
  "description": "An http server pretending to be CouchDB, for unit testing.",
  "homepage": "http://chris-l.github.io/mock-couch/",
  "main": "index.js",
  "engine": "node >= 0.12.0",
  "scripts": {
    "test": "npm run test:lint && npm run test:spec",
    "test:lint": "jshint lib/* test/* index.js",
    "test:spec": "jasmine test/*.spec.js"
  },
  "keywords": [
    "couchdb",
    "http",
    "server",
    "testing",
    "mock"
  ],
  "author": "Christopher Luna <dev@christopher-luna.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:chris-l/mock-couch.git"
  },
  "dependencies": {
    "couch-viewkey-compare": "^1.0.1",
    "ramda": "^0.8.0",
    "restify": "^2.8.5",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "jasmine": "^2.8.0",
    "jshint": "^2.9.5",
    "standard": "^10.0.3"
  },
  "jshintConfig": {
    "-W105": true
  }
}
